<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.edu.fjnu.cjrserver.dao.DemandTableForNameMapper">
    <!-- 朱仲策 -->
    <!--查询需求调查表通过名字-->
    <select id="findDemandTableByName" resultType="com.edu.fjnu.cjrserver.model.DemandTable">
        SELECT * from demand_table where demandtable_name like #{demandtable_name}
    </select>

</mapper>